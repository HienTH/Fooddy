@model FoodShop.CSDL.Group_Index
 @{
          Layout = "~/Views/Shared/_LayoutIndex.cshtml";
 }
        <div class="cup">
            @foreach (var item in @Model.LsCategory)
            {                
                <div class="col-md-4 cup-in" style="float:left; margin:0.7em;">
                    <a href="single.html"><img src="~/Content/images/@item.Image" style="width: 174px; height: 178.5px;"></a>
                <p>@item.Name</p>
                <div class="details-in">
                   @Html.ActionLink("Details", "Single", new { Name = item.ID }, new { @class = "details" })
                </div>
                <div class="clearfix"> </div>
                </div>
            }
            <div class="clearfix"> </div>
        </div>
